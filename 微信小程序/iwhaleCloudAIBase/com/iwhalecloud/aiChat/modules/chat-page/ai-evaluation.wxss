.evaluation-background-view {
	width: 600rpx;
	display: flex;
	align-items: center;
	flex-direction: column;
}

.evaluation-container {
	width: 100%;
  background: white;
	margin-top: -60rpx;
  border-radius: 20rpx;
}

.evaluation-gray-container {
	margin-top: 20rpx;
	background-color: #fafafa;
	width: 100%;
	height: 200rpx;
}

.evaluation-flex-view {
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
}

.evaluation-starRate{
	display: inline-block;
  width: 50rpx;
  height: 50rpx;
  margin-top: 35rpx;
	margin-left: 10rpx;
}
.evaluation-submitButton{
	width: 100%;
	color: white;
	background-color: #0B99DE;
	text-align: center;
	height: 80rpx;
	font-size: 40rpx;
	line-height:80rpx;
	margin-top: 40rpx;
	margin-bottom: 0rpx;  
}

.evaluation-btn1 {
	margin-top: 25rpx;
  margin-right: 10rpx;
  width: 470rpx;
  height: 70rpx;
  border: 1rpx solid #1e96f7; 
  border-radius: 10rpx;  
  background-color: #f5f9fd;
	text-align: left;
	line-height: 70rpx;
	padding-left: 10rpx;
	font-size: 35rpx;
	color: black;
}
.evaluation-iconimage {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  margin-top: 20rpx;
