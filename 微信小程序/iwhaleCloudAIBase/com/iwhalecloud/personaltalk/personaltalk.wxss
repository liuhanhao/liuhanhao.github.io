.tabbar{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.tabbar-item{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
}
.item-text{
  color: #333;
  font-size: 35rpx;
}
.item-line-view {
	background-color: #0a99df;
	width: 33.33%;
	height: 2rpx;
	margin-left: 0rpx;
}
.search-view{
  margin: 20rpx 30rpx;
}
.start-time{
  display: flex;
  flex-direction: row;
  padding: 10rpx 40rpx;
}
.start-time-text{
  font-size: 35rpx;
  color: #333;
}
.arrow-view{
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.arrow{
  height: 30rpx;
  width: 30rpx;
}

.item{
  
}

.itemTitleView {
  display: flex;
  margin-top: 20rpx;
}
.itemIconImage {
  width: 70rpx;
  height: 70rpx;
  display: inline-block;
  margin-left: 60rpx;
}
.itemTitle {
  display: inline-block;
  margin-left: 10rpx;
}
.itemLeftText {
  font-size: 35rpx;
  color: gray;
  padding-top: 20rpx;
  margin-left: 40rpx;
}
.itemRightText {
  font-size: 35rpx;
  color: gray;
  padding-top: 20rpx;
  margin-left: 20rpx;
}
.phoneImage {
  width: 40rpx;
  height: 50rpx;
  position: relative;
  left: 600rpx;
  top: -10rpx;
}
.evaluationButton {
  background-color: #0a99df;
  margin-right: 60rpx;
  margin-bottom: 20rpx;
  color: white;
  width: 180rpx;
  height: 70rpx;
  line-height: 2.0;
  font-size: 15px;
}
.moreButton {
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-bottom: 40rpx;
  color: #0a99df;
}