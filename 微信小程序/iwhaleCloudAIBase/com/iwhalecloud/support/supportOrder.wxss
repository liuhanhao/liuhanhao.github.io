.tabbar{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.tabbar-item{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
}
.item-text{
  color: #333;
  font-size: 35rpx;
}
.item-line-view {
	background-color: #0a99df;
	width: 50%;
	height: 2rpx;
	margin-left: 0rpx;
}
.search-view{
  margin: 20rpx 30rpx;
}
.start-time{
  display: flex;
  flex-direction: row;
  padding: 10rpx 40rpx;
}
.start-time-text{
  font-size: 35rpx;
  color: #333;
}
.arrow-view{
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.arrow{
  height: 30rpx;
  width: 30rpx;
}

.item{
  
}
.itemTopTitleView {
  margin-top: 20rpx;
  padding-top: 20rpx;
  display: flex;
}
.itemTitleView {
  margin-top: 20rpx;
  padding-top: 20rpx;
}
.threeItemTitleView {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.threeItemTopView {
  padding-top: 20rpx;
}
.itemIconImage {
  width: 70rpx;
  height: 70rpx;
  display: inline-block;
  margin-left: 60rpx;
}
.itemTitle {
  display: inline-block;
  margin-left: 10rpx;
}
.itemLeftText {
  font-size: 35rpx;
  color: gray;
  margin-left: 40rpx;
}
.itemRightText {
  font-size: 35rpx;
  color: gray;
  margin-left: 20rpx;
}
.spDescLeftText {
  font-size: 15px;
  color: black;
  padding-top: 20rpx;
  margin-left: 40rpx;
}
.spDescRightText {
  font-size: 14px;
  color: black;
  padding-top: 8.5px;
  margin-left: 20rpx;
}
.phoneImage {
  width: 40rpx;
  height: 50rpx;
  padding-right:30px;
}

.feekBackButton {
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 8px;
  height: 30px;
  text-align: center;
  font-size: 12px;
  background-color: white;
  border: 1px solid #0a99df;
  color: #0a99df;
}
.moreButton {
  margin-left: 40rpx;
  margin-right: 40rpx;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  color: #0a99df;
}


